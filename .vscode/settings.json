{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "clsx",
    "cmds",
    "dotenv",
    "hookform",
    "hygen",
    "labstack",
    "nextauth",
    "openapi",
    "tailwindcss",
    "tanstack",
    "taskfile",
    "tsnd",
    "typegen",
    "ulidx"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "cssVariables.lookupFiles": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less"
  ],
  "eslint.workingDirectories": ["./app/client"]
}
